-- // Logic
local running = false
local function log(msg)
    debugBox.Text = debugBox.Text .. "\n" .. msg
    debugBox.CursorPosition = #debugBox.Text
end

-- ‚úÖ ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏•‡∏∑‡∏≠‡∏Å Rod ‡πÇ‡∏î‡∏¢‡πÑ‡∏°‡πà‡∏™‡∏ô prefix/buff
local function getZenithRod()
    for _, tool in ipairs(backpack:GetChildren()) do
        if tool:IsA("Tool") and string.match(tool.Name, "Rod Of The Zenith") then
            return tool
        end
    end
    return nil
end

local function equipRod()
    local rod = getZenithRod()
    if rod then
        player.Character.Humanoid:EquipTool(rod)
        log("‡∏û‡∏ö Scylla ‚Üí ‡∏ñ‡∏∑‡∏≠ Rod Of The Zenith")
    else
        log("‚ùå ‡πÑ‡∏°‡πà‡∏°‡∏µ Rod Of The Zenith ‡πÉ‡∏ô Backpack")
    end
end

-- ‡∏õ‡∏∏‡πà‡∏°‡∏ó‡∏≥‡∏á‡∏≤‡∏ô
toggleBtn.MouseButton1Click:Connect(function()
    running = not running
    if running then
        toggleBtn.Text = "‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô"
        toggleBtn.BackgroundColor3 = Color3.fromRGB(220, 100, 100)
        log("‡πÄ‡∏£‡∏¥‡πà‡∏° Auto Scylla Hunt")
        -- ‡∏ï‡∏±‡∏ß‡∏≠‡∏¢‡πà‡∏≤‡∏á: ‡πÄ‡∏ß‡∏•‡∏≤ detect fish_Scylla
        equipRod()
    else
        toggleBtn.Text = "‡πÄ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô"
        toggleBtn.BackgroundColor3 = Color3.fromRGB(100, 220, 100)
        log("‡∏´‡∏¢‡∏∏‡∏î Auto Scylla Hunt")
    end
end)

-- ‡∏õ‡∏∏‡πà‡∏° Debug
debugBtn.MouseButton1Click:Connect(function()
    debugFrame.Visible = not debugFrame.Visible
end)

-- ‡∏õ‡∏∏‡πà‡∏° Copy
copyBtn.MouseButton1Click:Connect(function()
    if setclipboard then
        setclipboard(debugBox.Text)
        log("‚úÖ Debug log copied!")
    else
        StarterGui:SetCore("SendNotification", {Title="Error", Text="setclipboard ‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ", Duration=3})
    end
end)

-- ‡∏õ‡∏∏‡πà‡∏° Minimize
local minimized = false
minimizeBtn.MouseButton1Click:Connect(function()
    minimized = not minimized
    if minimized then
        mainFrame.Size = UDim2.new(0, 260, 0, 40)
        toggleBtn.Visible = false
        debugBtn.Visible = false
        footer.Visible = false
    else
        mainFrame.Size = UDim2.new(0, 260, 0, 180)
        toggleBtn.Visible = true
        debugBtn.Visible = true
        footer.Visible = true
    end
end)

-- ‡∏õ‡∏∏‡πà‡∏° Close
closeBtn.MouseButton1Click:Connect(function()
    screenGui:Destroy()
end)
